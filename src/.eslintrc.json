{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "modules": true,
          "experimentalObjectRestSpread": true
      }
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "comma-dangle": 0,
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "react/prop-types": 0,
    "no-unused-vars": "warn",
    "no-unexpected-multiline": "warn",
    "no-console": "warn"
  },
  "settings": {
    "react": {
        "pragma": "React",
        "version": "16.2.0"
    }
  }
}
